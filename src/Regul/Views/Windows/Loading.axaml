<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="60"
		Width="300" Height="60"
        x:Class="Regul.Views.Windows.Loading"
		Background="Transparent"
		TransparencyLevelHint="Transparent"
		CanResize="False"
		SystemDecorations="None"
		ShowInTaskbar="False"
		WindowStartupLocation="CenterOwner">
	<Border Classes="BorderStyle">
		<StackPanel VerticalAlignment="Center" Margin="30 0">
			<TextBlock x:Name="processText" HorizontalAlignment="Center"/>
			<ProgressBar x:Name="progress" Maximum="100"/>
		</StackPanel>
	</Border>
</Window>
